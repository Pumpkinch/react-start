{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/function-component-definition": 0,
        "react/jsx-filename-extension": 0,
        "jsx-a11y/control-has-associated-label": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react/prop-types": 0
    }
}
